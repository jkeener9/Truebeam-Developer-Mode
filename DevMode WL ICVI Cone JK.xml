<VarianResearchBeam SchemaVersion="1.0">
  <!--*********************************-->
  <!--Dev Mode uses Varian standard machine scale -->
  <!--Must Park MLC and retract kV Arms -->
  <!--*********************************-->
  <SetBeam>
    <!--Couch values are represented in Isocentric parameterization-->
    <Id>1234</Id>
    <MLCModel>NDS120HD</MLCModel>
    <Accs>
    </Accs>
    <ControlPoints>
      <Cp>
        <!--control point 0, Gantry head up.-->
        <SubBeam>
          <Seq>0</Seq>
          <Name>MV Outside</Name>
        </SubBeam>
        <Energy>6xFFF</Energy>
        <Mu>0</Mu>
        <DRate>600</DRate>
        <GantryRtn>180.00</GantryRtn>
        <CollRtn>180.0</CollRtn>
        <CouchRtnISO>180</CouchRtnISO>
        <Y1>2.5</Y1>
        <Y2>2.5</Y2>
        <X1>2.5</X1>
        <X2>2.5</X2>
      </Cp>
      <!--5 gantry angle (270,0.1,359.9,180.0, 90) with couch at 180.-->
      <Cp>
        <!--control point 1.-->
      </Cp>
      <Cp>
        <!--control point 2.-->
        <GantryRtn>90.00</GantryRtn>
      </Cp>
      <Cp>
        <!--control point 3.-->
        <GantryRtn>0.10</GantryRtn>
      </Cp>
      <Cp>
        <!--control point 4.-->
        <GantryRtn>270.00</GantryRtn>
      </Cp>
      <Cp>
        <!--control point 5.-->
        <GantryRtn>359.9</GantryRtn>
      </Cp>
      <!--return gantry to head up-->
      <Cp>
        <!--control point 6. no image-->
        <GantryRtn>180.00</GantryRtn>
      </Cp>
      <!--Couch Rotations with gantry head up.-->
      <Cp>
        <!--control point 7.-->
        <CouchRtnISO>90</CouchRtnISO>
      </Cp>
      <Cp>
        <!--control point 8.-->
        <CouchRtnISO>135</CouchRtnISO>
      </Cp>
      <Cp>
        <!--control point 9.-->
        <CouchRtnISO>225</CouchRtnISO>
      </Cp>
      <Cp>
        <!--control point 10.-->
        <CouchRtnISO>270</CouchRtnISO>
      </Cp>
      <!-- Return couch to center.-->
      <Cp>
        <!--control point 11. no image-->
        <CouchRtnISO>180</CouchRtnISO>
      </Cp>
      <!-- Collimator Rotations-->
      <Cp>
        <!--control point 12.-->
        <CollRtn>135</CollRtn>
      </Cp>
      <Cp>
        <!--control point 13.-->
        <CollRtn>90</CollRtn>
      </Cp>
      <Cp>
        <!--control point 14.-->
        <CollRtn>225</CollRtn>
      </Cp>
      <Cp>
        <!--control point 15.-->
        <CollRtn>270</CollRtn>
      </Cp>
      <!--RESET Collimator.-->
      <Cp>
        <!--control point 16. no image-->
        <CollRtn>180</CollRtn>
      </Cp>
    </ControlPoints>
    <!--
******************************************************************* 
 Here is where we specify that outside treatment mode 
 ********************************************************************-->
    <ImagingParameters>
      <OutsideTreatment>
        <MaxMu>100</MaxMu>
        <!-- This is just a limit of max 
MU to be delivered. Actual MU delivered will be just what is enough to take the MV 
image we are requesting.-->

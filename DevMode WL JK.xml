				</Mlc>
			</Cp>
			<!--5 gantry angle (270,0.1,359.9,180.0, 90) with couch at 180.-->
			<Cp>
				<!--control point 1.-->
			</Cp>
			<Cp>
				<!--control point 2.-->
				<GantryRtn>90.00</GantryRtn>
			</Cp>
			<Cp>
				<!--control point 3.-->
				<GantryRtn>0.10</GantryRtn>
			</Cp>
			<Cp>
				<!--control point 4.-->
				<GantryRtn>270.00</GantryRtn>
			</Cp>
			<Cp>
				<!--control point 5.-->
				<GantryRtn>359.9</GantryRtn>
			</Cp>
			<!--return gantry to head up-->
			<Cp>
				<!--control point 6. no image-->
				<GantryRtn>180.00</GantryRtn>
			</Cp>
			<!--Couch Rotations with gantry head up.-->
			<Cp>
				<!--control point 7.-->
				<CouchRtn>90</CouchRtn>
			</Cp>
			<Cp>
				<!--control point 8.-->
				<CouchRtn>135</CouchRtn>
			</Cp>
			<Cp>
				<!--control point 9.-->
				<CouchRtn>225</CouchRtn>
			</Cp>
			<Cp>
				<!--control point 10.-->
				<CouchRtn>270</CouchRtn>
			</Cp>
			<!-- Return couch to center.-->
			<Cp>
				<!--control point 11. no image-->
				<CouchRtn>180</CouchRtn>
			</Cp>
			<!-- Collimator Rotations-->
			<Cp>
				<!--control point 12.-->
				<CollRtn>135</CollRtn>
			</Cp>
			<Cp>
				<!--control point 13.-->
				<CollRtn>90</CollRtn>
			</Cp>
			<Cp>
				<!--control point 14.-->
				<CollRtn>225</CollRtn>
			</Cp>
			<Cp>
				<!--control point 15.-->
				<CollRtn>270</CollRtn>
			</Cp>
			<!--RESET Collimator.-->
			<Cp>
				<!--control point 16. no image-->
				<CollRtn>180</CollRtn>
			</Cp>
		</ControlPoints>
